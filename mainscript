local GameSelection = Games:NewSection("Scripts For Games")

GameSelection:NewButton("Vehicle Simulator", "vehicle simulator script, speed, autofarm etc.", function()
    loadstring(game:HttpGet("https://pastebin.com/raw/HFkLb2Pw"))()
end)

