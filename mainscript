local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Alpha Hub V4", "Ocean")

-- Home
local Home = Window:NewTab("Home")
local HomeSelection = Home:NewSection("                   Welcome To Alpha Hub V5!!!")

-- Misc
local Misc = Window:NewTab("Misc")
local MiscSection = Misc:NewSection("Misc Scripts")

MiscSection:NewButton("Infinite Yield", "this script makes you quite an admin bruv", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
end)

MiscSection:NewButton("Dex Explorer", "you can see dev things", function()
    loadstring(game:HttpGet("https://cdn.wearedevs.net/scripts/Dex%20Explorer.txt"))()
end)

MiscSection:NewButton("Infinite jump", "you can break your spacebar", function()
    loadstring(game:HttpGet("https://cdn.wearedevs.net/scripts/Infinite%20Jump.txt"))()
end)

MiscSection:NewButton("Chat Spoofer", "lets you chat for other people", function()
    loadstring(game:HttpGet("https://pastebin.com/raw/djBfk8Li"))()
end)

MiscSection:NewSlider("Walk Speed", "gotta go fast!", 200, 16, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

MiscSection:NewSlider("Jump Power", "gotta go high!", 200, 50, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
end)



-- Aimbot
local Aimbot = Window:NewTab("Aimbot")
local AimbotSelection = Aimbot:NewSection("Aimbot Scripts")

AimbotSelection:NewButton("Owl Hub", "nah, he just have good gaming chair", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/CriShoux/OwlHub/master/OwlHub.txt"))()
end)

AimbotSelection:NewButton("Dark Hub", "nah, he just have good gaming chair v2", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/RandomAdamYT/DarkHub/master/Init", true))()
end)


