GameSelection:NewButton("Blox Burg", "script for blox burg", function()
    loadstring(game:HttpGet(('https://pastebin.com/raw/jCunLjFE'),true))()
end)

GameSelection:NewButton("Meep City +", "enables plus", function()
    game.ReplicatedStorage.PlayerData[game.Players.LocalPlayer.UserId].PLUS.Value = true
end)

